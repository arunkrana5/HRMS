    @Html.IncludeVersionedCss(Url.Content("~/assets/design/plugins/web-fonts/plugin.css"))
    @Html.IncludeVersionedCss(Url.Content("~/assets/design/plugins/web-fonts/plugin.css"))


    @Html.IncludeVersionedCss(Url.Content("~/assets/design/css/select2.min.css"))
    @Html.IncludeVersionedCss(Url.Content("~/assets/design/css/custom.css"))
    @Html.IncludeVersionedCss(Url.Content("~/assets/design/css/customdgn.css"))
    @Html.IncludeVersionedCss(Url.Content("~/assets/design/plugins/morris/morris.css"))


    <!--------------navbar--------------------------->

    @Html.IncludeVersionedCss(Url.Content("~/assets/design/css/navbars.css"))

    <!---Fontawsome 6 -->
    @Html.IncludeVersionedCss(Url.Content("~/assets/design/plugins/fontawesome-free-6.2.0-web/css/all.min.css"))



    <!--------------File manager--------------------------->
    @Html.IncludeVersionedCss(Url.Content("/Content/ej2/material.css"))

    @RenderSection("style", required: false)

    <!-------------------------HRMS StyleSheet Main Shweta Dhanvantri-->
    @Html.IncludeVersionedCss(Url.Content("~/assets/design/css/hrmsResponsive.css"))
